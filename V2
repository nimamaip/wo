#include<stdio. h>
#include<string . h>
void main( )
{
    char str[]="2*2/4*1/1*2*3/2";
	int i,num=str[0]-'0' ;
    for(i= 0;i<strlen(str);i++)
    {
        if(str[i]=='*')
           num=num*(str[i+1]-'0');
	    if(str[i]=-'/')
	       num=num/(str[i+1]-'0');
    }
    printf("jieguo=%d\n",num) ;
}
